{
  "manifest_version": 3,
  "name": "Face Swap Extension",
  "version": "1.0",
  "description": "使用RunPod Inswapper API進行人臉交換的Chrome擴展插件",
  "permissions": [
    "contextMenus",
    "storage",
    "notifications",
    "offscreen",
    "downloads"
  ],
  "host_permissions": [
    "https://api.runpod.ai/*",
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}